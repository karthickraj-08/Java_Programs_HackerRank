import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String n=sc.nextLine();
        int len=0;
         char[] ch = n.toCharArray();
        for (char c : ch) {
            len++;
        }
        System.out.println(len);
    }
}
