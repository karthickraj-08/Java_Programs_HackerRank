import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int start = scanner.nextInt();
        int end = scanner.nextInt();

        for(int i=start; i<=end; i++) {
            if(i % 2 == 0) {
                System.out.print(i + " ");
            }
        }
    }
}
